import <std/time> <std/memory>

namespace std {
    short randomShort(short min, short max) {
        std::timeSpec ts;
        std::getCurrentTime(&ts);
    } => ((cast(short)ts.nsec) % ((max+1)-min))+min;

    int randomInt(int min, int max) {
        std::timeSpec ts;
        std::getCurrentTime(&ts);
    } => ((cast(int)ts.nsec) % ((max+1)-min))+min;
}