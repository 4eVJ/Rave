import <std/time>

namespace libc {
    extern(linkname: "srand") void srand(int seed);
    extern(linkname: "rand") int rand();
}

namespace std {
    int randomInt(int min, int max) {
        int result = 0;

        std::timeSpec ts;
        libc::clockGetTime(std::RealTime,&ts);
        libc::srand(cast(int)ts.nsec);

        result = libc::rand();
    } => (result % ((max+1)-min))+min;
}