@once

isdigit(c: char): bool {
    ret (c >= '0') && (c <= '9');
}

isalpha(c: char): bool {
    ret ((c >= 'a') && (c <= 'z')) || ((c >= 'A') && (c <= 'Z'));
}

islower(c: char): bool {
    ret ((c >= 'a') && (c <= 'z'));
}

isupper(c: char): bool {
    ret !islower(c);
}

toupper(c: char): char {
    if(islower(c)) {
        ret toret - 'a' + 'A';
    }
    ret toret;
}

@end