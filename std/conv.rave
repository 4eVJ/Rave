import <std/string>

namespace std {
    Out fromTo<In,Out>(In n) {
        Out t;

        @if(@tEquals(In,Out)) {
            t = n;
        };
        @else {
            @if(@isNumeric(In)) {
                @if(@isNumeric(Out)) {
                    t = cast(Out)n;
                };
            };
        };
    } => t;
}