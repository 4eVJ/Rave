import <std/mem>
namespace std {
    struct string {
        char* ptr;
        int len;

        std::string this(char* a) {
            this.len = std::strlen(a);
            this.ptr = std::malloc(this.len+1);

            int i = 0;
            while(i<this.len) {
                this.ptr[i] = a[i];
                i += 1;
            }
            this.ptr[i] = '\0';
        }

        void clear {
            delete(this.ptr);
        }
    }
}