import <std/io>

int A(vararg args) {
    int toret = 0;

    int i = 0;
    while(i<args.length) {
        if(sizeof(int) == args.vars[i].size) {
            toret += cast(int)args.vars[i].value;
        }
        i += 1;
    }
} => toret;

int main {
    vararg args;

    args.length = 2;
    args.vars[0].value = cast(void*)5;
    args.vars[0].size = sizeof(int);
    args.vars[1].value = cast(void*)7;
    args.vars[1].size = sizeof(int);
} => A(args);